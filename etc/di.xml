<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="\Magento\CatalogImportExport\Model\Import\Product\Validator">
        <plugin name="firegento_extendedimport_validator" type="\FireGento\ExtendedImport\Plugin\ValidatorPlugin"
                sortOrder="1" disabled="false"/>
        <plugin name="firegento_extendedimport_createmissingattributeoption"
                type="\FireGento\ExtendedImport\Plugin\CreateMissingAttributeOptionPlugin" sortOrder="2"
                disabled="false"/>
    </type>

    <type name="Magento\ImportExport\Model\Import\Config">
        <plugin name="firegento_extendedimport_override_url_key_validation" type="FireGento\ExtendedImport\Plugin\AfterGetEntities"/>
    </type>


    <preference for="Magento\CatalogImportExport\Model\Import\Product\CategoryProcessor"
                type="FireGento\ExtendedImport\Model\Magento\CatalogImportExport\Import\Product\CategoryProcessor"
    />
</config>
